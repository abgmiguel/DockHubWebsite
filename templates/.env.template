# MongoDB connection
MONGODB_URI=mongodb://admin:{{MONGO_PASSWORD}}@127.0.0.1:{{MONGODB_PORT}}/{{DB_NAME}}?authSource=admin

# Backend server port  
PORT={{BACKEND_PORT}}

# JWT secret for authentication
JWT_SECRET={{JWT_SECRET}}

# CORS origin for frontend
CORS_ORIGIN={{FRONTEND_URL}}

# MongoDB root password (for docker-compose)
MONGO_PASSWORD={{MONGO_PASSWORD}}

# Environment
NODE_ENV=production

# Backend port for internal API calls
BACKEND_PORT={{BACKEND_PORT}}